<div class="row text-center py-5">
            {{#each data}}
            <div class="col-lg-3 col-md-6 col-sm-12">
                <img src="./{{imagepath}}" alt="Coffee" 
                onmouseover="setActiveImagepath(this, `{{imagepath}}`)"
                onmouseout="setActiveImagepath(this, `{{imagepath}}`, false)">
                <h3>{{name}}</h3>
                <p class="text-black-50">{{Summary}}
                </p> 
             {{!-- <a class="btn btn-link readmore pt-0" href="#" onclick="loadData(`products/{{id}}`,'details-template','content')">Readmore</a> --> --}}
            <a class="btn btn-link readmore pt-0" href="?request=products/{{id}}">Readmore</a>
            </div>
            {{/each}}
        </div>